<!doctype html>
<html lang="ja">
<head>
	<meta charset="utf-8">
	<title>oidc sample rp - attr</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<style>
		table#attr, table#attr th, table#attr td { border: solid 1px #C0C0C0; border-collapse: collapse; }
	</style>
</head>
<body>
	<table id="attr">
		<tr>
			<th>Claim</th>
			<th>Value</th>
		</tr>
<% Object.entries(idtoken).forEach(([key, value])=> { %>
        <tr>
            <td><%=key%></td>
            <td><%=value%></td>
        </tr>
<% }); %>
	</table>
</body>
</html>
